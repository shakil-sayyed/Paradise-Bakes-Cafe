{
  "name": "paradise-bakes-cafe-backend",
  "version": "1.0.0",
  "description": "Backend API for Paradise Bakes & Cafe",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "migrate": "ts-node ./scripts/migrate.ts",
    "seed": "ts-node ./scripts/seed.ts"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.3",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.0",
    "mysql2": "^3.2.0"
