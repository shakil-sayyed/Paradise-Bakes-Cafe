{
  "name": "paradise-bakes-cafe",
  "version": "1.0.0",
  "description": "Paradise Bakes & Cafe - Pure Vegetarian Delights Web Application",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "install-server": "npm install",
    "install-all": "npm run install-server && npm run install-client",
    "heroku-postbuild": "npm run install-client && npm run build",
    "db:setup": "node scripts/setupDatabase.js",
    "db:seed": "node scripts/seedData.js",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "paradise-bakes-cafe",
    "vegetarian",
    "cafe",
    "food-delivery",
    "pwa",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "Shakil Sayyed",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "redis": "^4.6.8",
    "nodemailer": "^6.9.4",
    "qrcode": "^1.5.3",
    "moment": "^2.29.4",
    "uuid": "^9.0.0",
    "sharp": "^0.32.5",
    "express-session": "^1.17.3",
    "connect-mongo": "^5.0.0",
    "pm2": "^5.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.2",
    "supertest": "^6.3.3",
    "eslint": "^8.47.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
